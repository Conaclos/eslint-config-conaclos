{
  "name": "eslint-config-conaclos",
  "version": "1.2.0",
  "title": "Conaclos's ESLint config",
  "description": "ESLint shareable configuration from Conaclos",
  "homepage": "https://github.com/Conaclos/eslint-config-conaclos#readme",
  "license": "Zlib",
  "author": "Victorien Elvinger (victorien.elvinger.fr)",
  "repository": {
    "type": "git",
    "url": "https://github.com/Conaclos/eslint-config-conaclos.git"
  },
  "bugs": {
    "url": "https://github.com/Conaclos/eslint-config-conaclos/issues"
  },
  "files": [
    "index.js",
    "base.js",
    "esnext-base.js",
    "style.js",
    "esnext-style.js",
    "browser-only-mod.js",
    "node-only-mod.js",
    "dist-mod.js"
  ],
  "main": "index.js",
  "keywords": [
    "eslintconfig",
    "eslint",
    "linter",
    "jslint",
    "jshint",
    "jscs",
    "checking",
    "checker",
    "quality",
    "rules",
    "enforcement",
    "prevention",
    "code",
    "safer",
    "better",
    "style",
    "strict",
    "adaptable",
    "modular"
  ],
  "scripts": {
    "lint": "eslint -c style.js ./*.js && eslint -c esnext-style.js ./test",
    "release": "standard-version --sign",
    "test:unit": "ava",
    "test": "npm run lint && npm run test:unit"
  },
  "config": {
    "ghooks": {
      "commit-msg": "opt --out commit-msg --exec validate-commit-msg",
      "pre-push": "opt --out pre-push --exec 'npm test'"
    }
  },
  "engines": {
    "node": ">=4.2.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "eslint": "^3.7.0",
    "eslint-plugin-extra-rules": "^0.6.1",
    "eslint-plugin-promise": "^2.0.1",
    "ghooks": "^1.3.2",
    "opt-cli": "^1.5.1",
    "standard-version": "^2.4.0",
    "temp-write": "^2.1.0",
    "validate-commit-msg": "^2.8.0"
  },
  "peerDependencies": {
    "eslint": ">=3.4.0",
    "eslint-plugin-promise": ">=2.0.1",
    "eslint-plugin-extra-rules": ">=0.6.1"
  }
}
