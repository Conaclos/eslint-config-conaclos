{
  "name": "eslint-config-conaclos",
  "version": "2.4.0",
  "title": "Conaclos's ESLint config",
  "description": "ESLint shareable configuration from Conaclos",
  "homepage": "https://github.com/Conaclos/eslint-config-conaclos#readme",
  "license": "Zlib",
  "author": "Victorien Elvinger (victorien.elvinger.fr)",
  "repository": {
    "type": "git",
    "url": "https://github.com/Conaclos/eslint-config-conaclos.git"
  },
  "bugs": {
    "url": "https://github.com/Conaclos/eslint-config-conaclos/issues"
  },
  "files": [
    "index.js",
    "base.js",
    "esnext-base.js",
    "ts-base.js",
    "style.js",
    "esnext-style.js",
    "ts-style.js",
    "browser-only-mod.js",
    "node-only-mod.js",
    "dist-mod.js"
  ],
  "main": "index.js",
  "keywords": [
    "eslintconfig",
    "eslint",
    "linter",
    "jslint",
    "jshint",
    "jscs",
    "checking",
    "checker",
    "quality",
    "rules",
    "enforcement",
    "prevention",
    "code",
    "safer",
    "better",
    "style",
    "strict",
    "adaptable",
    "modular"
  ],
  "scripts": {
    "lint": "eslint -c  style.js ./*.js &&  eslint -c esnext-style.js ./test",
    "release": "standard-version --sign",
    "test": "npm run lint && ava"
  },
  "engines": {
    "node": ">=4.2.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm test"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^7.3.2",
    "@commitlint/config-conventional": "^7.3.1",
    "@typescript-eslint/eslint-plugin": "^1.0.0",
    "@typescript-eslint/parser": "^1.0.0",
    "ava": "^1.0.0",
    "eslint": "^5.12.1",
    "husky": "^1.3.1",
    "standard-version": "^4.4.0",
    "temp-write": "^3.4.0",
    "typescript": "^3.2.4"
  },
  "peerDependencies": {
    "eslint": ">=5.12.1",
    "typescript": ">=3.2.0",
    "@typescript-eslint/parser": ">=1.0.0",
    "@typescript-eslint/eslint-plugin": ">=1.0.0"
  }
}
